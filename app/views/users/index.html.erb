<h1>ユーザー</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>ユーザー名</th>
      <th>登録日</th>
    </tr>
  </thead>

  <tbody>
    <% @users .each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= link_to user.username, user %></td>
        <td><%= user.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
